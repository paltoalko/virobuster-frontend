{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": ["tsconfig.json"]
  },
  "plugins": ["react"],
  "rules": {
    "semi": "off",
    "@typescript-eslint/semi": ["error"],
    "react/jsx-filename-extension": [
      2,
      { "extensions": [".js", ".jsx", ".ts", ".tsx"] }
    ]
  }
}
